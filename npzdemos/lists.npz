import std;

func listString(lst) {
  let s = "[";
  let n = std.length(lst);

  for (let i = 0; i < std.length(lst); i = i + 1) {
    s = s + std.asString(lst[i]);
    if (i < n - 1)
      s = s + ", ";
  }

  return s + "]";
}

// Creating lists
let list = [1, 2, 3, 4, 5];
std.println(listString(list));

// Setting indices
list[2] = 17;
std.println(listString(list));

// Appending
std.append(list, "hello!");
std.println(listString(list));

// Removing
remove(list, 1);
std.println(listString(list));

// Popping
std.println(std.pop(list));
std.println(listString(list));

