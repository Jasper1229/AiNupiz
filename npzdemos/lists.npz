
func listString(lst) {
  let s = "[";
  let n = length(lst);

  for (let i = 0; i < length(lst); i = i + 1) {
    s = s + asString(lst[i]);
    if (i < n - 1)
      s = s + ", ";
  }

  return s + "]";
}

// Creating lists
let list = [1, 2, 3, 4, 5];
println(listString(list));

// Setting indices
list[2] = 17;
println(listString(list));

// Appending
append(list, "hello!");
println(listString(list));

// Removing
remove(list, 1);
println(listString(list));

// Popping
println(pop(list));
println(listString(list));

